{
    "editor.accessibilitySupport": "off",
    "editor.cursorWidth": 2,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.formatOnType": true,
    "editor.minimap.maxColumn": 100,
    "editor.quickSuggestionsDelay": 3,
