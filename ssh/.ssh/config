# ssh settings

# Default config
Host *
    AddressFamily any
    Compression yes
    ConnectionAttempts 2
    # Necessary for zsh completion
    HashKnownHosts no
    IdentityFile ~/.ssh/id_rsa
    NumberOfPasswordPrompts 3
    AddKeysToAgent yes
    UseKeychain yes

Host github.com
    User git
    IdentitiesOnly yes

Host heroku.com
    User git
    IdentitiesOnly yes

Host gitlab.com
    User git
    IdentitiesOnly yes

Host gitlab.dev.ifs.hsr.ch
    User git
    IdentitiesOnly yes

Host tenantmanager
    HostName tenantmanager.ins.local
    User sysadmin

Host tenantmanager.ins.local
    User sysadmin