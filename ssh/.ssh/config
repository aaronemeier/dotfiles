# ssh settings

# Default config
Host *
    AddressFamily any
    Compression yes
    ConnectionAttempts 2
    # Necessary for zsh completion
    HashKnownHosts no
    IdentityFile ~/.ssh/id_rsa
    NumberOfPasswordPrompts 3
    AddKeysToAgent yes
    UseKeychain yes

Include ~/.dotfiles/work_configs/ssh/config
Include ~/.dotfiles/sync/ssh/config
